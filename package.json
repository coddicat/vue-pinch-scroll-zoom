{
  "name": "@coddicat/vue-pinch-scroll-zoom",
  "version": "4.3.5",
  "description": "Vue component enabling scroll and zoom functionalities for content, leveraging mouse interactions on desktop and two-finger pinch gestures on mobile devices",
  "author": "Rodion Shlomo Solomonyk",
  "homepage": "https://github.com/coddicat/vue-pinch-scroll-zoom",
  "keywords" : ["scroll", "zoom", "scale", "mouse", "pinch", "vue", "mobile", "web"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/coddicat/vue-pinch-scroll-zoom.git"
  },
  "type": "module",
  "main": "./dist/pinch-scroll-zoom.umd.js",
  "module": "./dist/pinch-scroll-zoom.js",
  "types": "./dist/components/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/pinch-scroll-zoom.js",
      "require": "./dist/pinch-scroll-zoom.umd.js",
      "types": "./dist/components/index.d.ts"
    },
    "./style.css": "./dist/style.css"
