{
  "name": "@coddicat/vue-pinch-scroll-zoom",
  "version": "4.7.1",
  "description": "Vue component enabling scroll and zoom functionalities for content, leveraging mouse interactions on desktop and two-finger pinch gestures on mobile devices",
  "author": "Rodion Shlomo Solomonyk",
  "homepage": "https://github.com/coddicat/vue-pinch-scroll-zoom",
  "keywords": [
    "scroll",
    "zoom",
    "scale",
    "mouse",
    "pinch",
    "vue",
    "mobile",
    "web"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/coddicat/vue-pinch-scroll-zoom.git"
  },
  "type": "module",
  "main": "./dist/pinch-scroll-zoom.umd.js",
  "module": "./dist/pinch-scroll-zoom.js",
  "types": "./dist/components/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/pinch-scroll-zoom.js",
      "require": "./dist/pinch-scroll-zoom.umd.js",
      "types": "./dist/components/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist/**/*"
  ],
  "style": "dist/style.css",
  "scripts": {
    "dev": "vite",
    "host": "vite --host",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@types/lodash": "^4.17.0",
    "lodash": "^4.17.21",
    "sass": "^1.74.1",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.24.1",
    "prettier": "^2.8.8",
    "typescript": "^5.4.4",
    "vite": "^4.5.3",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.8.27"
  }
}
